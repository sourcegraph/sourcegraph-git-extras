{
  "$schema": "https://raw.githubusercontent.com/sourcegraph/sourcegraph/main/client/shared/src/schema/extension.schema.json",
  "name": "git-extras",
  "publisher": "sourcegraph",
  "title": "Git extras",
  "description": "A Sourcegraph extension that adds useful features when viewing files in a Git repository on Sourcegraph, GitHub, GitLab, and other supported code hosts.",
  "icon": "data:image/png;base64,",
  "activationEvents": [
    "*"
  ],
  "tags": [
    "blame",
    "github",
    "gitlab",
    "code host",
    "author"
  ],
  "categories": [
    "Code analysis"
  ],
  "contributes": {
    "actions": [
      {
        "id": "git.blame.toggle",
        "command": "updateConfiguration",
        "commandArguments": [
          [
            "git.blame.decorations"
          ],
          "${(config.git.blame.decorations === 'line' && 'file') || (config.git.blame.decorations === 'file' && 'none') || 'line'}",
          null
        ],
        "category": "Git",
        "title": "${(config.git.blame.decorations === 'line' && 'Show blame for the whole file') || (config.git.blame.decorations === 'file' && 'Hide blame') || 'Show blame for selected lines'}",
        "actionItem": {
          "label": "Blame",
          "description": "${(config.git.blame.decorations === 'line' && 'Show Git blame line annotations for the whole file') || (config.git.blame.decorations === 'file' && 'Hide Git blame line annotations') || 'Show Git blame line annotations on selected lines'}",
          "pressed": "(config.git.blame.decorations === 'line') || (config.git.blame.decorations === 'file')",
          "iconURL": "https://raw.githubusercontent.com/sourcegraph/sourcegraph-git-extras/63dd95962c43b95b3f3a9ea2aa0165d6b38a958c/icon/git_logo.svg?sanitize=true"
        }
      }
    ],
    "menus": {
      "editor/title": [
        {
          "action": "git.blame.toggle",
          "when": "resource && clientApplication.isSourcegraph"
        }
      ],
      "commandPalette": [
        {
          "action": "git.blame.toggle",
          "when": "resource"
        }
      ]
    },
    "configuration": {
      "title": "Git extras",
      "properties": {
        "git.blame.decorations": {
          "description": "Whether to decorate all lines in a file, only selected lines, or none at all.",
          "type": "string",
          "enum": [
            "none",
            "line",
            "file"
          ],
          "default": "none"
        },
        "git.blame.showPreciseDate": {
          "description": "Whether to show precise dates (e.g. \"Mar 17, 2021\"). By default, distance from current date is shown (e.g. \"3 months ago\")",
          "type": "boolean",
          "default": false
        }
      }
    }
  },
  "version": "0.0.0-DEVELOPMENT",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sourcegraph/sourcegraph-git-extras.git"
  },
  "files": [
    "dist"
  ],
  "main": "dist/sourcegraph-git-extras.js",
  "scripts": {
    "prettier-check": "npm run prettier -- --write=false",
    "prettier": "prettier '**/*.{js?(on),ts?(x),scss,yml,md}' --write --list-different",
    "tslint": "tslint -c tslint.json -p tsconfig.json './src/*.ts?(x)' './*.ts?(x)'",
    "typecheck": "tsc -p .",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --require ts-node/register --require source-map-support/register --opts mocha.opts",
    "cover": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' nyc --require ts-node/register --require source-map-support/register --all mocha --opts mocha.opts --timeout 10000",
    "build": "parcel build --no-minify --out-file sourcegraph-git-extras.js src/extension.ts",
    "symlink-package": "mkdirp dist && lnfs ./package.json ./dist/package.json",
    "serve": "npm run symlink-package && parcel serve --no-hmr --out-file sourcegraph-git-extras.js src/extension.ts",
    "sourcegraph:prepublish": "yarn typecheck && yarn test && yarn build"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.ts?(x)"
    ],
    "exclude": [
      "**/*.test.ts?(x)",
      "src/util/stubs.ts"
    ],
    "extension": [
      ".tsx",
      ".ts"
    ]
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Edge versions",
    "last 1 Safari versions"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@sourcegraph/prettierrc": "^2.2.0",
    "@sourcegraph/tsconfig": "^4.0.1",
    "@sourcegraph/tslint-config": "^13.4.0",
    "@types/expect": "1.20.4",
    "@types/lodash": "4.17.13",
    "@types/mocha": "9.1.1",
    "@types/node": "22.10.1",
    "@types/sinon": "17.0.3",
    "expect": "^30.2.0",
    "graphql": "^15.4.0",
    "husky": "^4.3.5",
    "lnfs-cli": "^2.1.0",
    "lodash": "^4.17.20",
    "mkdirp": "^0.5.1",
    "mocha": "^6.1.4",
    "nyc": "^13.3.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "sinon": "^17.0.1",
    "source-map-support": "^0.5.12",
    "sourcegraph": "^25.7.0",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "typescript": "^4.1.2",
    "vscode-languageserver-types": "^3.14.0"
  },
  "dependencies": {
    "date-fns": "^2.0.0-alpha.24",
    "rxjs": "^6.6.3",
    "tagged-template-noop": "^2.1.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  }
}
